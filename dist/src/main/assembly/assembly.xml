<assembly>
  <formats>
    <format>zip</format>
  </formats>
  <dependencySets>
    <dependencySet>
      <useTransitiveDependencies>false</useTransitiveDependencies>
      <outputDirectory>/lib</outputDirectory>
      <includes>
        <include>*:*</include>
        <include>org.eclipse.osgi:cm</include>
      </includes>
      <excludes>
        <exclude>org.eclipse:*</exclude>
        <exclude>org.eclipse.osgi:services</exclude>
        <exclude>org.eclipse.osgi:util</exclude>
        <exclude>org.ops4j.pax.runner:*</exclude>
      </excludes>
    </dependencySet>
    <dependencySet>
      <outputDirectory>/bin</outputDirectory>
      <includes>
        <include>org.ops4j.pax.runner:*</include>
      </includes>
    </dependencySet>
  </dependencySets>
  <repositories>
    <repository>
      <outputDirectory>equinox</outputDirectory>
      <includes>
        <include>org.eclipse:*</include>
        <include>org.eclipse.osgi:*</include>
      </includes>
    </repository>
  </repositories>
  <fileSets>
    <fileSet>
      <directory>src/main/etc</directory>
      <fileMode>0755</fileMode>
      <outputDirectory></outputDirectory>
    </fileSet>
  </fileSets>
</assembly>
